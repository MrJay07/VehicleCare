	<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>

	<link rel="stylesheet" href="css/service.css" type="text/css" media="all" />

	<script type="text/javascript">
		

		var query = new URLSearchParams(window.location.search);
		var tab = query.get('tab');
		$('a[href="#'+ tab +'"]').tab('show')
		$type = '';
		$(document).scroll(function() {
		  var $self = $("#fixedDivScroll");
		  $self.css('margin-top', 0);
		  var fixedDivOffset = $self.offset().top + $self.outerHeight(true);
		  if (fixedDivOffset > ($("#testi").offset().top - 30)) {
		    $self.css('margin-top', -(fixedDivOffset - $("#testi").offset().top));
			$('#serviceNav').hide();
		  } else {
		    $self.css('margin-top', '7.5em');
			$('#serviceNav').show();
		  }
		});

		
		
		$('#serviceNav a.nav-link').on('shown.bs.tab', function(e) {
		  var href = $(this).attr('href');
		  $('html, body').animate({
		    scrollTop: $(href).offset().top-400
		  }, 1000);
		});

		$body = $('body');
		$cartArray = [];
		$priceServiceArray = [];
		$carDetails = {};
		const citySelectedName = readCookie('selectedCityName');
		if (citySelectedName){
			$( ".serviceCity" ).empty();
			$( ".serviceCity" ).append(citySelectedName);
		}
		$(document).ready(function() {
			localStorage.removeItem('cartArray');
			localStorage.removeItem('carDetails');
			const citySelected = readCookie('selectedCityName');
			var url_string = window.location.href;
			var url = new URL(url_string);
			var tab = url.searchParams.get('tab');
			var	brandId = readCookie("selected_carBrandName")
			var	modelId = readCookie("selected_carBrandModel")
			// Redirect to Mobile View if from mobile
			if(window.innerWidth <= 800){
				location.href = `serviceMobile.php?city=${citySelectedName}&brandName=${brandId}&modelName=${modelId}`;
				return false;
			};
			var isExist = readCookie('userMobile');
			if(isExist && isExist != ''){
				const params = {
					carBrand: brandId,
					carModel: modelId,
					mobile: isExist
				};
				newPendingEntry(params);
			}
			

			if (citySelected){
				if(!brandId || !modelId ){
					changeCarBrand();
					$('#brandModelModal').modal('show');
					return false;
				}
			}


			
			// $('#brandModelModal').modal('show');
			// getCarDetails({brandId:brandId, modelId:modelId});

			$carDetails = {brandId, modelId	};
			getCarDetails({brandId, modelId}, (response) => {
				result = JSON.parse(response);
				$type = result.type;
				$('#carDetails').html(`${result.brand} ${result.model}`);
			});
			if (modelId) {
				getPrices({modelId}, (response) => {
					result = JSON.parse(response);
					$priceServiceArray = result;
					$.each(result, function(key,val){
						var serviceId = val.service_name;
						var price = val.price || 'Not Available';
						var id = val.id;
						$('#'+serviceId.toLowerCase().replace(/\//g, "").replace(/\)/g, "").replace(/\(/g, "").replace(/ /g, "_"))
							.html('&#8377;'+price).next('button').attr({'data-id': id, 'data-price': price, 'disabled': false});
					});
				});
			}
			var couponResponse = localStorage.getItem('couponData');
			if(couponResponse){
				couponResponse = JSON.parse(couponResponse);
				if(couponResponse && couponResponse.code){
					$('#couponCode').val(couponResponse.code);
				}
			}
		});
		// $('#brandModelModal').on('hidden.bs.modal', () => {
		// 	var url_string = window.location.href;
		// 	var url = new URL(url_string);
		// 	var brandId = url.searchParams.get("bid");
		// 	var modelId = url.searchParams.get("mid");
		// 	if(!brandId || !modelId){
		// 		location.href = 'index.php';
		// 		return false;
		// 	}
		// })
		$body.on('click', '#applyCoupon', (event) => {
			event.preventDefault();
			var code = $('#couponCode').val().trim();
			if(!code) return false;
			var misc_data = JSON.stringify($type);
			validateCouponCode({code, source: 'flipkart', misc_data}, (response) => {
				localStorage.setItem('couponData', response);
				response = JSON.parse(response);
				if(response && response.code){
					alert('Coupon Code applied..!!');
				}else{
					$('#couponCode').val('');
					alert('Not a Valide Coupon Code..!!');
				}
				showCartService();
			});
		});

		function changeCarBrand(){
			getBrandNames((response) => {
				var $selectCarBrand = $('.car_brand');
				result = JSON.parse(response);
				$selectCarBrand.html('');
				$selectCarBrand.append('<option value="">Select Your Brand</option>');

				$.each(result, function(key,val){
					var brand = val.car_brand;
					var id = val.id;
					$selectCarBrand.append('<option name="'+brand+'" value="'+id+'">' + brand + '</option>');
				});
			});
		}

		function onBrandChange(id, element) {
			getModelNames(id, (response) => {
				$selectCarModel = $(element);
				result = JSON.parse(response);
				$selectCarModel.html('');
				$selectCarModel.append('<option value="">Select Your Model</option>');

				$.each(result, function(key,val){
					var brand = val.model_name;
					var id = val.id;
					$selectCarModel.append('<option name="'+brand+'" value="'+id+'">' + brand + '</option>');
				});
			});
		}
		
		const getNewServiceList = (id, element) => {
			var carBrand = $(element).val().trim();
			getCarDetails({brandId: carBrand, modelId: id}, (response) => {
				result = JSON.parse(response);
				location.href = `service.php?city=${citySelectedName}&brandName=${result.brand.replace(/ /g,"_")}&modelName=${result.model.replace(/ /g,"_")}`;
			});
		}
		const showCartService = () => {
			var totalPrice = 0;
			var discountVal = 0;
			if($cartArray.length){
				$selectedCart = $('#cartItems');
				$selectedCart.html('');
				$.each($cartArray, function(key,val){
					var name = val.service_name;
					var price = val.price;
					var id = val.id;
					totalPrice += parseInt(price, 0);
					$selectedCart.append('<div class="selectedItem pt-2 pb-2"><span data-id="'+id+'" class="removeCart"><i class="fa fa-trash text-danger mr-2"></i></span><span class="serviceName">'+name+'</span><span class="pull-right">&#8377; '+price+'</span></div>');
				});
				var couponResponse = localStorage.getItem('couponData');
				if(couponResponse){
					couponResponse = JSON.parse(couponResponse);
					if(couponResponse && couponResponse.code){
						$('#couponCode').val(couponResponse.code);
						var discountParam = couponResponse.value;
						discountVal = discountParam;
						if(discountParam.includes("%")){
							discountVal = parseFloat((totalPrice * parseFloat(discountParam))/100).toFixed(2);
						}
					}
				}
				$('#checkoutCart').prop('disabled', false);
			}else{
				$('#cartItems').html('<h6 class="mb-3 text-center text-danger pt-3">No Service Selected</h6>');
				$('#checkoutCart').prop('disabled', true);
			}
			totalPrice -= discountVal;
			$('.discount').html('&#8377; '+discountVal);
			$('.grandAmount').html('&#8377; '+totalPrice);
		}
		$body.on('click', '.changeCar', () => {
			$('.cartShowModalBrand').toggleClass('isHidden');
		});
		$body.on('click', '.removeCart', function() {
			const data = $(this).data();
			$('.addToCart.selected[data-id="'+data.id+'"]').removeClass('selected').html('+ Add to cart');
			$cartArray = $cartArray.filter((service) => parseInt(service.id, 0) !== data.id);
			showCartService();
		});
		$body.on('click', '.addToCart.selected', function() {
			const data = $(this).data();
			$(this).removeClass('selected').html('+ Add to cart');
			$cartArray = $cartArray.filter((service) => parseInt(service.id, 0) !== data.id);
			showCartService();
		});
		$body.on('click', '.addToCart:not(".selected")', function() {
			const data = $(this).data();
			if(!data.price || data.price =='Not Available'){
				alert('This Service is not available. Kindly contact to Customer Care to opt this Service.');
				return false;
			}
			$(this).addClass('selected').html('- Remove from cart');
			var serviceArray = $priceServiceArray.filter((service) => parseInt(service.id, 0) === data.id);
			$cartArray.push(...serviceArray);
			showCartService();
		});
		$body.on('click', '#checkoutCart', (event) => {
			event.preventDefault();
			const citySelectedName = readCookie('selectedCityName');
			localStorage.setItem('cartArray', JSON.stringify($cartArray));
			localStorage.setItem('carDetails', JSON.stringify($carDetails));
			location.href = `book.php?city=${citySelectedName}`;
		});
		function shareFunction() {
			var $temp = $("<input>");
			var $url = $(location).attr('href');
			$("body").append($temp);
			$temp.val($url).select();
			document.execCommand("copy");
			$temp.remove();
			alert('Now you can paste the link to share!!');
		}
	</script>
</body>

</html>

