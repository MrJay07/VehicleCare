					appId: "AIzaSyA1maZ4DWkW4jn2HCy7lze3pgpWCvZatqc"
				};
				// Initialize Firebase
				var firebaseApp = firebase.initializeApp(firebaseConfig);
				firebase.auth().onAuthStateChanged(function (user) {
					updateSignInButtonUI();
				});
				window.recaptchaVerifier = new firebase.auth.RecaptchaVerifier('getOTP', {
					'size': 'invisible',
					'callback': function (response) {
						console.log(response);
						onSignInSubmit();
					}
				});

				window.recaptchaVerifier.render().then((widgetId) => {
					window.recaptchaWidgetId = widgetId;
					updateSignInButtonUI();
				});
				document.getElementById('en_mobile').addEventListener('keyup', updateSignInButtonUI);
				document.getElementById('en_mobile').addEventListener('change', updateSignInButtonUI);
			}
			function updateSignInButtonUI() {
				var email = $('#en_mobile').val().trim();
				if (!email) {
					document.getElementById('getOTP').disabled = true;
				} else {
					document.getElementById('getOTP').disabled = false;
				}
			}
			$(document).ready(function () {
				var isExist = readCookie('userMobile');
				if (isExist && isExist != '') {
					createCookie('userMobile', '', 0);
				}
				getAllCarBrands();
				getAllFaqListing();
				$('#primeModal').modal('show');
				setTimeout(function () { $('#primeModal').modal('hide'); }, 10000);
			});

			var url_string = window.location.href;
			var url = new URL(url_string);

			const getAllCarBrands = () => {
				getBrandNames((response) => {
					result = JSON.parse(response);
					$selectCarBrand.html('');
					$selectCarBrand.append('<option value="">Select Your Brand</option>');

					$.each(result, function (key, val) {
						var brand = val.car_brand;
						var id = val.id;
						$selectCarBrand.append('<option name="' + brand + '" value="' + id + '">' + brand + '</option>');
					});
				});
			}
			const getAllFaqListing = () => {
				const citySelected = readCookie('selectedCity');
				const element = $('#accordion.faqList');
				getFaqsNames(['0', citySelected], (response) => {
					result = JSON.parse(response);
					element.html('');
					$.each(result, function (key, val) {
						element.append(createFaqCells(key, val));
					});
				});
			}
			const getAllCarModels = (id, $selectCarModel) => {
				createCookie('selected_carBrandName', id, 1);
				getModelNames(id, (response) => {
					result = JSON.parse(response);
					$selectCarModel.html('');
					$selectCarModel.append('<option value="">Select Your Model</option>');

					$.each(result, function (key, val) {
						var brand = val.model_name;
						var id = val.id;
						$selectCarModel.append('<option name="' + brand + '" value="' + id + '">' + brand + '</option>');
					});
				});
			}
			const makeSubmitActive = (id) => {
				$(id).find('input[type="submit"]').prop('disabled', false);
			}
			$body.on('submit', '#selectBrandModel', (event) => {
				event.preventDefault();
				var carBrand = $('.car_brand').val().trim();
				var carModel = $('.car_model_price').val().trim();
				var mobile = $('#mobile').val().trim();
				var target = 'VC Homepage';
				city = citySelectedName;
				if (!city) { city = "city"; }
				createCookie('userMobile', mobile, 10);
				saveServiceQuery({ mobile, target, carBrand, carModel, city }, (response) => {
					getCarDetails({ brandId: carBrand, modelId: carModel }, (response) => {
						result = JSON.parse(response);
						location.href = `service.php?city=${citySelectedName}&brand=${result.brand.replace(/ /g, "_")}&model=${result.model.replace(/ /g, "_")}`;
					});
				});

			});
			$body.on('submit', '#serviceQueryForm', (event) => {
				event.preventDefault();
				var username = $('#username').val().trim();
				var mobile = $('#mobile').val().trim();
				var captcha = $('#captcha').val().trim();
				var carBrand = $('.car_brand.query').val().trim();
				var carModel = $('.car_model_quotes').val().trim();
				if (!username || !mobile || !captcha || !carBrand || !carModel) {
					alert('All Fields are Mandatory');
					return false;
				}
				createServiceQuery({ username, mobile, captcha, carBrand, carModel }, (response) => {
					if (response === 'false') {
						alert('Captcha is incorrect');
						$('#captcha').focus();
						return false;
					}
					$('#serviceQueryForm').find('input:not(".submit"), select').val('');
					$('#sccessModal').modal('show');
				});
			});
			$body.on('submit', '#getEnrollCode', (event) => {
				event.preventDefault();
				var mobile = $('#en_mobile').val().trim();
				var carReg = $('#carReg').val().trim();
				if (!mobile || !carReg) {
					alert('All Fields are Mandatory');
					return false;
				}
				var appVerifier = window.recaptchaVerifier;
				firebase.auth().signInWithPhoneNumber('+91' + mobile, appVerifier)
					.then(function (confirmationResult) {
						window.confirmationResult = confirmationResult;
						$('#SubmitCheck').modal('show');
					}).catch(function (error) {
						console.log(error);
						alert(error.message || 'Something went wrong. Try again.');
					});
			});
			$body.on('click', '#submitOtp', (event) => {
				event.preventDefault();
				var otp = $('#inputOtp').val();
				var car_reg = $('#carReg').val().trim();
				var mobile = $('#en_mobile').val().trim();
				const params = { mobile, car_reg };
				confirmationResult.confirm(otp).then(function (result) {
					getEnrollCode(params, res => {
						res = JSON.parse(res);
						if (res && res.code) {
							alert(`Your Enroll Benefit code is ${res.code}`);
							return false;
						} else {
							alert(`Details doesn't match`);
							return false;
						}
					});
				}).catch(function (error) {
					console.log(error);
					if (error.code == "auth/invalid-verification-code") {
						alert("Please enter a valid code or request a new code by selecting resend.");
					} else {
						alert('Something went wrong. Try again.');
					}
				});
			});
			$('#sccessModal').on('hidden.bs.modal', () => location.reload());
		</script>


