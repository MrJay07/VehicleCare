		<script>
			$body = $('body');
			$leftTime = 1598356740;
			const citySelectedName = readCookie('selectedCityName');

			$selectCarBrand = $('.car_brand.list');
			window.onload = function () {
				var firebaseConfig = {
					apiKey: "AIzaSyA1maZ4DWkW4jn2HCy7lze3pgpWCvZatqc",
					authDomain: "AIzaSyA1maZ4DWkW4jn2HCy7lze3pgpWCvZatqc",
					databaseURL: "AIzaSyA1maZ4DWkW4jn2HCy7lze3pgpWCvZatqc",
					projectId: "AIzaSyA1maZ4DWkW4jn2HCy7lze3pgpWCvZatqc",
